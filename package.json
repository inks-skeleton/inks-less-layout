{
  "name": "layout-simple",
  "version": "2.0.0",
  "description": "layout-simple是为web前端响应式布局而生的CSS布局框架，基于LESS开发，可实现灵活配置、自动生成符合需求的布局框架、自动LESS变量、可用于PC、Mobile或响应式布局等web开发。",
  "private": true,
  "scripts": {
    "build:gulp": "cross-env MODE_TYPE=gulp webpack --config config/webpack.prod.js",
    "build:loader": "cross-env MODE_TYPE=loader webpack --config config/webpack.prod.js",
    "test:gulp": "gulp",
    "test:loader": "cross-env MODE_TYPE=testWLess webpack --config config/webpack.test.js",
    "dev": "cross-env MODE_TYPE=dev webpack-dev-server --config config/webpack.test.js"
  },
  "author": "YanEr",
  "license": "ISC",
  "dependencies": {
    "gulp-util": "^3.0.8",
    "loader-utils": "^1.2.3",
    "merge-objects": "^1.0.5",
    "through2": "^3.0.1",
    "tmp": "^0.1.0",
    "webpack": "^4.41.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@types/gulp-util": "^3.0.34",
    "@types/loader-utils": "^1.1.3",
    "@types/merge-objects": "^1.0.0",
    "@types/node": "^13.1.0",
    "@types/request": "^2.48.4",
    "@types/tapable": "^1.0.4",
    "@types/through2": "^2.0.34",
    "@types/tmp": "^0.1.0",
    "@types/webpack": "^4.41.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-less": "^4.0.1",
    "gulp-minify-css": "^1.2.4",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^5.0.0",
    "less-plugin-autoprefix": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "keywords": [
    "gulp",
    "css",
    "web",
    "layout-simple"
  ],
  "homepage": "https://github.com/qc-web-y/layout-simple",
  "repository": {
    "type": "git",
    "url": "https://github.com/qc-web-y/layout-simple.git"
  },
  "bugs": {
    "url": "https://github.com/qc-web-y/layout-simple/issues"
  }
}
